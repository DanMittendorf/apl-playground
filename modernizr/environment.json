{
  "type": "APL",
  "version": "1.3",
  "settings": {},
  "theme": "dark",
  "import": [],
  "resources": [],
  "styles": {},
  "onMount": [],
  "graphics": {},
  "commands": {},
  "layouts": {
    "Environment": {
      "parameters": [],
      "item": {
        "type": "CustomFrame",
        "headline": "Environment",
        "items": [
          {
            "type": "Option",
            "label": "agentName",
            "isBoolean": false,
            "value": "${environment.agentName}",
            "description": "Name of the runtime environment"
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "agentVersion",
            "isBoolean": false,
            "value": "${environment.agentVersion}",
            "description": "Version of the runtime environment"
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "allowOpenURL",
            "value": "${environment.allowOpenURL}",
            "description": "True if the OpenURL command is enabled"
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "animation",
            "isBoolean": false,
            "value": "${environment.animation}",
            "description": "Animation characteristics of the runtime"
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "aplVersion",
            "isBoolean": false,
            "value": "${environment.aplVersion}",
            "description": "Supported version of APL."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "disallowVideo",
            "value": "${environment.disallowVideo}",
            "description": "True if the Video component is disabled"
          }
        ]
      }
    }
  }
}