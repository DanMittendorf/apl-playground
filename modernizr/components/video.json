{
  "type": "APL",
  "version": "1.3",
  "settings": {},
  "theme": "dark",
  "import": [],
  "resources": [],
  "styles": {},
  "onMount": [],
  "graphics": {},
  "commands": {},
  "layouts": {
    "VideoComponent": {
      "parameters": [],
      "item": {
        "type": "CustomFrame",
        "headline": "Video",
        "componentId": "componentsNavigation",
        "items": [
          {
            "type": "Option",
            "label": "audioTrack",
            "value": true,
            "description": "Audio track to play on."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "autoplay",
            "value": true,
            "description": "If true, automatically start playing the video."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "onEnd",
            "value": true,
            "description": "Commands to execute when the last video track is finished playing."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "onPause",
            "value": true,
            "description": "Commands to execute when the video switches from playing to paused."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "onPlay",
            "value": true,
            "description": "Commands to execute when the video switches from paused to playing."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "onTimeUpdate",
            "value": "${environment.aplVersion >= '1.1'}",
            "description": "Commands to execute when the playback position changes."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "onTrackUpdate",
            "value": true,
            "description": "Commands to execute when the current video track changes."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "scale",
            "value": true,
            "description": "How the video should scale to fill the space."
          },
          {
            "type": "Separator"
          },
          {
            "type": "Option",
            "label": "source(s)",
            "value": true,
            "description": "Video source or sources."
          }
        ]
      }
    }
  }
}